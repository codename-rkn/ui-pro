var chart_loader = function(){
    const chart = new window.Charts.entryChart('chart-entries');

    chart.initializeChart();

    const data = {
        labels: <%= data[:entry_names].keys.to_json.html_safe %>,
        entryNames: <%= data[:entry_names].values.to_json.html_safe %>
    };

    chart.updateDataset(data);
};

$(document).ready(chart_loader);
